{
  "name": "rocket-weather",
  "version": "0.0.1",
  "description": "A minimalistic weather app with automatic gps location tracking",
  "productName": "Rocket Weather",
  "author": "Francesco Silvani",
  "private": true,
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
